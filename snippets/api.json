        "body": [
            "FUNCTION ${1|uint8,uint16,uint32,uint64,int8,int16,int32,int64,double,string,file,le_result_t,le_onoff_t|} ${2:functionName}",
            "(",
            "\t$0",
            ");",
            ""
        ]
    },
